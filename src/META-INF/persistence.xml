<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="libreria_appPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>libreria_app.User</class>
    <class>libreria_app.Celular</class>
    <class>libreria_app.TipoCaracteristicas</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:oracle:oci8:@localhost:1521:orcl"/>
      <property name="javax.persistence.jdbc.user" value="ruby"/>
      <property name="javax.persistence.jdbc.driver" value="oracle.jdbc.driver.OracleDriver"/>
      <property name="javax.persistence.jdbc.password" value="ruby"/>
    </properties>
  </persistence-unit>
</persistence>
